{
  "name": "justgiving-text-totals",
  "version": "0.1.0",
  "description": "Updates a text file with JustGiving Donation Data",
  "main": "app.js",
  "scripts": {
    "start": "electron .",
    "package": "electron-packager ./ JGTextTotals --arch all --platform win32 --asar --icon app/img/app --out .release --version 1.4.3 --overwrite",
    "package-all": "electron-packager ./ JGTextTotals --all --asar --icon app/img/app --out .release --version 1.4.3 --overwrite"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lolPants/justgiving-text-totals.git"
  },
  "keywords": [
    "justgiving",
    "text",
    "api",
    "electron"
  ],
  "author": "Jack Baron <me@jackbaron.com> (https://jackbaron.com)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lolPants/justgiving-text-totals/issues"
  },
  "homepage": "https://github.com/lolPants/justgiving-text-totals#readme",
  "devDependencies": {
    "electron": "^1.4.3",
    "electron-packager": "^8.1.0"
  },
  "dependencies": {
    "electron-json-storage": "^2.0.1",
    "jquery": "^3.1.1",
    "request": "^2.75.0"
  }
}
