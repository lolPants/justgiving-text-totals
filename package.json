{
  "name": "justgiving-text-totals",
  "version": "0.7.0",
  "description": "Updates a text file with JustGiving Donation Data",
  "main": "src/",
  "scripts": {
    "start": "electron .",
    "package": "electron-packager ./ JGTextTotals --arch x64 --platform win32 --platform linux --asar --icon app/img/app --out .release --version 1.6.2 --overwrite",
    "package-quick": "electron-packager ./ JGTextTotals --arch x64 --platform win32 --asar --icon app/img/app --out .release --version 1.6.2 --overwrite",
    "package-all": "electron-packager ./ JGTextTotals --all --asar --icon app/img/app --out .release --version 1.6.2 --overwrite"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lolPants/justgiving-text-totals.git"
  },
  "keywords": [
    "justgiving",
    "text",
    "api",
    "electron"
  ],
  "author": "Jack Baron <me@jackbaron.com> (https://jackbaron.com)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lolPants/justgiving-text-totals/issues"
  },
  "homepage": "https://github.com/lolPants/justgiving-text-totals#readme",
  "devDependencies": {
    "electron-json-storage": "^3.0.4",
    "eslint": "^3.19.0",
    "jquery": "^3.2.1",
    "request": "^2.81.0"
  },
  "dependencies": {
    "electron": "^1.6.2",
    "electron-packager": "^8.6.0"
  }
}
